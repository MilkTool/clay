    println(0b01111111_11111111_11111111_11111111_11111111_11111111_11111111_11111111l);
    //println(0b01111111_11111111_11111111_11111111_11111111_11111111_11111111_11111111_11111111_11111111_11111111_11111111_11111111_11111111_11111111_11111111ll);

